{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": [],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false,
    "systemName"            : "DADI API CouchDB Adapter",
    "footer"                : "{string}",
    "copyright"             : "{string}",
    "includeDate"           : false,
    "navType"               : "inline",
    "theme"                 : "journal",
    "linenums"              : true,
    "collapseSymbols"       : true,
    "inverseNav"            : false,
    "outputSourceFiles"     : true ,
    "outputSourcePath"      : false,
    "syntaxTheme"           : "default",
    "sort"                  : true
  }
}
